{
  "mcpServers": {
    "swe-swe-agent-chat": {
      "command": "npx",
      "args": ["-y", "@choonkeat/agent-chat"]
    },
    "swe-swe-playwright": {
      "command": "npx",
      "args": ["-y", "@playwright/mcp@latest", "--cdp-endpoint", "http://chrome:9223"]
    },
    "swe-swe-preview": {
      "command": "/repos/agent-reverse-proxy/workspace/dist/agent-reverse-proxy",
      "args": ["--tool-prefix", "preview", "--theme-cookie", "swe-swe-theme"]
    },
    "swe-swe-whiteboard": {
      "command": "npx",
      "args": ["-y", "@choonkeat/agent-whiteboard"]
    }
  }
}
