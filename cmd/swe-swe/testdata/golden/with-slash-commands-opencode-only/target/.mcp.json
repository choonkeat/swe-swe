{
  "mcpServers": {
    "swe-swe-agent-chat": {
      "args": [
        "-y",
        "@choonkeat/agent-chat"
      ],
      "command": "npx"
    },
    "swe-swe-playwright": {
      "args": [
        "-y",
        "@playwright/mcp@latest",
        "--cdp-endpoint",
        "http://chrome:9223"
      ],
      "command": "npx"
    },
    "swe-swe-preview": {
      "args": [
        "--mcp"
      ],
      "command": "swe-swe-server"
    },
    "swe-swe-whiteboard": {
      "args": [
        "-y",
        "@choonkeat/agent-whiteboard"
      ],
      "command": "npx"
    }
  }
}
